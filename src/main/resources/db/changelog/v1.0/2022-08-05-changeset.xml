<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:pro="http://www.liquibase.org/xml/ns/pro"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

	<changeSet author="andrewsha" id="users-addColumn-phone_number">
		<preConditions onFail="MARK_RAN">
			<tableExists tableName="users" />
			<not>
				<columnExists tableName="users" columnName="phone_number" />
			</not>
		</preConditions>
		<addColumn catalogName="DatabaseChangelog"
			schemaName="public" tableName="users">
			<column afterColumn="email" name="phone_number" type="varchar(255)"></column>
		</addColumn>
	</changeSet>

</databaseChangeLog>